<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="nofas_keluar_v2_tree" model="ir.ui.view">
		<field name="name">Laporan Pengeluaran Tree</field>
		<field name="model">djbc.nofas_keluar_v2</field>
		<field name="arch" type="xml">
			<tree string="Laporan Pengeluaran" create="0" edit="0">
				<field name="jenis_dok"/>
				<field name="no_aju" invisible="True"/>
				<field name="tgl_aju" invisible="True"/>
				<field name="no_dok"/>
				<field name="tgl_dok"/>
				<field name="no_pengeluaran"/>
				<field name="tgl_pengeluaran"/>
				<field name="no_bl" invisible= "True"/>
				<field name="tgl_bl" invisible="True"/>
				<field name="no_cont" invisible= "True"/>
				<field name="penerima"/>
				<field name="pemilik" invisible= "True"/>
				<field name="hs_code" invisible= "True" />
				<field name="kode_barang"/>
				<field name="nama_barang"/>
				<field name="jumlah"/>
				<field name="satuan"/>
				<field name="jumlah_kemasan" invisible="True"/>
				<field name="satuan_kemasan" invisible="True"/>
				<field name="nilai"/>
				<field name="currency"/>
				<field name="location" invisible="True"/>
				<field name="warehouse"/>
				<field name="alm_wh" invisible="True"/>
				<field name="kota_wh" invisible="True"/>
			</tree>
		</field>
	</record>

	<record id="nofas_keluar_v2_form" model="ir.ui.view">
		<field name="name">Laporan Pengeluaran Form</field>
		<field name="model">djbc.nofas_keluar_v2</field>
		<field name="arch" type="xml">
			<form create="0" edit="0">
				<group>
					<group>
						<field name="jenis_dok"/>
						<field name="no_aju"/>
						<field name="tgl_aju"/>
						<field name="no_dok"/>
						<field name="tgl_dok"/>
						<field name="no_pengeluaran"/>
						<field name="tgl_pengeluaran"/>
						<field name="no_bl"/>
						<field name="tgl_bl"/>
						<field name="no_cont"/>
						<field name="penerima"/>
						<field name="pemilik"/>
						<field name="hs_code"/>
					</group>
					<group>
						<field name="kode_barang"/>
						<field name="nama_barang"/>
						<field name="jumlah"/>
						<field name="satuan"/>
						<field name="jumlah_kemasan"/>
						<field name="satuan_kemasan"/>
						<field name="nilai"/>
						<field name="currency"/>
						<field name="location"/>
						<field name="warehouse"/>
						<field name="alm_wh"/>
						<field name="kota_wh"/>
					</group>
				</group>
			</form>
		</field>
	</record>

	<record id="nofas_keluar_v2_action" model="ir.actions.act_window">
		<field name="name">Laporan Pengeluaran</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">djbc.nofas_keluar_v2</field>
		<field name="view_mode">tree,form</field>
	</record>
</odoo>