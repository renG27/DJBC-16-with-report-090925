<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="nofas_masuk_tree" model="ir.ui.view">
            <field name="name">Laporan Pemasukan Tree</field>
            <field name="model">djbc.nofas_masuk_v4</field>
            <field name="arch" type="xml">
                <tree create="0" edit="0">
                    <field name="jenis_dok"/>
                    <field name="no_dok"/>
                    <field name="tgl_dok"/>
                    <field name="no_penerimaan"/>
                    <field name="tgl_penerimaan"/>
                    <field name="pengirim"/>
                    <field name="kode_barang"/>
                    <field name="nama_barang"/>
                    <field name="jumlah"/>
                    <field name="satuan"/>
                    <field name="nilai"/>
                    <field name="currency"/>
                    <field name="warehouse"/>
                </tree>
            </field>
        </record>

        <record id="nofas_masuk_form" model="ir.ui.view">
            <field name="name">Laporan Pemasukan Form</field>
            <field name="model">djbc.nofas_masuk_v4</field>
            <field name="arch" type="xml">
                <form create="0" edit="0">
                    <sheet>
                        <group>
                            <group>
                                <field name="jenis_dok"/>
                                <field name="no_dok"/>
                                <field name="tgl_dok"/>
                                <field name="no_penerimaan"/>
                                <field name="tgl_penerimaan"/>
                                <field name="pengirim"/>
                            </group>
                            <group>
                                <field name="kode_barang"/>
                                <field name="nama_barang"/>
                                <field name="jumlah"/>
                                <field name="satuan"/>
                                <field name="nilai"/>
                                <field name="currency"/>
                                <field name="warehouse"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="nofas_masuk_action" model="ir.actions.act_window">
            <field name="name">Laporan Pemasukan</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">djbc.nofas_masuk_v4</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>